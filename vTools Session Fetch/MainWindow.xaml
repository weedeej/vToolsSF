<Window x:Class="vTools_Session_Fetch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vTools_Session_Fetch"
        mc:Ignorable="d"
        Background="#21325E"
        ResizeMode="NoResize"
        Title="vTools Session Fetch" Height="450" Width="800">
    <ScrollViewer Name="ScrollView" VerticalScrollBarVisibility="Auto" CanContentScroll="True" ScrollChanged="ScrollView_ScrollChanged">
        <StackPanel Orientation="Vertical" Name="Output_Panel" Loaded="Output_Panel_Loaded">
        </StackPanel>
    </ScrollViewer>
</Window>
